@model IEnumerable<WebApplication3.Models.Tichet>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/AdminView.cshtml";

}

@section usage{
    <link href="~/css/uniform.default.css" rel="stylesheet" />
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DescriereTichet)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Urgenta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CnpStudent)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdAdministrator)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.DescriereTichet)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Urgenta)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CnpStudentNavigation.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IdAdministratorNavigation.Nume)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.IdTichet">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.IdTichet">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.IdTichet">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
